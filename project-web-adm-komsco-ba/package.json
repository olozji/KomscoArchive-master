{
  "name": "project-structure",
  "author": "Nextinnovation inc",
  "description": "Next Project Structure",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "homepage": "GIT_REPO_URL",
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=8.15.0",
    "yarn": "please-use-npm"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "scripts": {
    "serve": "env-cmd -f .env.local next dev",
    "serve-development": "env-cmd -f .env.development next dev",
    "serve-production": "env-cmd -f .env.production next dev",
    "build-export": "env-cmd -f .env.local next build && next export",
    "build-export-development": "env-cmd -f .env.development next build && next export",
    "build-export-production": "env-cmd -f .env.production next build && next export",
    "build": "env-cmd -f .env.local next build",
    "build-development": "env-cmd -f .env.development next build",
    "build-production": "env-cmd -f .env.production next build",
    "start-pm2": "pm2 start ecosystem.config.js --env local",
    "start-pm2-development": "pm2 start ecosystem.config.js --env development",
    "start-pm2-production": "pm2 start ecosystem.config.js --env production",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@types/react": "^18.0.17",
    "axios": "^0.27.2",
    "env-cmd": "^10.1.0",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.10",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
